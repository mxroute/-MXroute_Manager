            </form>
        </div>
        <div id="footerContainer">
            <div id="footer">
                <p>&nbsp;</p>
            </div>
        </div>
    </div>
</div>

<div id="subright">
   <div id="midtoolbar" class="clear">
        <div id="srtoolbar">
           <ul>
               <li id="action-home" onclick="return actionHandler('home');">
                   <a href="javascript:void(0)" class="btn btn-link">
                       <span class="fa fa-home fa-lg"></span>
                       [% locale.maketext("Home") %]
